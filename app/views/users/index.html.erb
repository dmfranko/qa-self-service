<div class="container">
	<div class="page-header">
		<h1>Users</h1>
	</div>

	<table class="table table-striped table-bordered" id="example">
		<tr>
			<th>Name</th>
			<th>Netid</th>
			<th>Admin</th>
		</tr>
		<% @u.each do |r| %>
		<tr>
			<td><%= r.name %></td>
			<td><%= r.netid %></td>
			<td><%= r.admin %></td>
		</tr>
		<% end %>
	</table>
</div>