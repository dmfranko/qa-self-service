<table class="table table-striped table-bordered" id="example">
	<tr>
		<th>Test ID</th>
		<th>Desc</th>
		<th>Status</th>
		<th class="hidden-xs">Passed</th>
		<th class="hidden-xs">Failed</th>
		<th class="hidden-xs">Pending</th>
		<th>Start</th>
		<th>Duration</th>
		<th class="hidden-xs">Notes</th>
		<th>Source</th>
		<th>Local/Remote</th>
		<th>OS</th>
		<th>Browser</th>
		<th>Version</th>
	</tr>
	<% @r.each do |r| %>
	<tr>
		<td><%= link_to r.id, run_detail_path(:id =>r.id) %></td>
		<td><%= r.description %></td>
		<td><%= r.status %></td>
		<td class="hidden-xs">1</td>
		<td class="hidden-xs">2</td>
		<td class="hidden-xs">3</td>
		<td><%= r.created_at.strftime("%m-%d-%y %r") %></td>
		<td><%= r.duration %></td>
		<td class="hidden-xs"><%= r.notes %></td>
		<td>Self Service</td>
		<td>Local</td>
		<td><%= r.os %></td>
		<td><%= r.browser %></td>
		<td><%= r.browser_version %></td>
	</tr>
	<% end %>
</table>
