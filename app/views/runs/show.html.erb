<div class="container">
	<div class="page-header">
		<h1>Test Execution <small>Run history</small></h1>
	</div>

	<%= link_to "Back", apps_path,class: "btn btn-default" %>
	<br />

	<table class="table table-striped table-bordered" id="example">
		<tr>
			<th>Test ID</th>
			<th>Desc</th>
			<th>Status</th>
			<th>Passed</th>
			<th>Failed</th>
			<th>Pending</th>
			<th>Start</th>
			<th>Duration</th>
			<th>Notes</th>
			<th>Source</th>
			<th>Local/Remote</th>
			<th>Platforms</th>
		</tr>
		<% @r.each do |r| %>
		<tr>
			<td><%= link_to r.id, run_detail_path(:id =>r.id) %></td>
			<td><%= r.description %></td>
			<td><%= r.status %></td>
			<td>1</td>
			<td>2</td>
			<td>3</td>
			<td><%= r.created_at.strftime("%m-%d-%y %r") %></td>
			<td><%= r.duration %></td>
			<td><%= r.notes %></td>
			<td>Self Service</td>
			<td>Local</td>
			<td>Windows Chrome, Mac Safari</td>
		</tr>
		<% end %>
	</table>
</div>