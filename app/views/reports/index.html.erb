<div class="container">
	<div class="page-header">
		<h1>DONT USE THIS <small>Run history</small></h1>
	</div>

	<table class="table table-striped table-bordered" id="example">
		<tr>
			<th>Desc</th>
			<th>Test ID</th>
			<th>Status</th>
			<th>Passed</th>
			<th>Failed</th>
			<th>Pending</th>
			<th>Start Time</th>
			<th>Duration</th>
			<th>Notes</th>
			<th>Show</th>
		</tr>
		<% @r.each do |r| %>
		<tr>
			<td><%= r.description %></td>
			<td><%= r.test_id %></td>
			<td><%= r.status %></td>
			<td>1</td>
			<td>2</td>
			<td>3</td>
			<td><%= r.created_at %></td>
			<td><%= r.duration %></td>
			<td><%= r.notes %></td>
			<td><%= link_to "Show", :controller => "run_details", :action => "show", :id => r %></td>

		</tr>
		<% end %>
	</table>
</div>
