<div class="container">
	<div class="page-header">
		<h1>My Apps <small>View Results and run tests</small></h1>
	</div>

	<table class="table table-striped table-bordered" id="example">
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Last status</th>
			<th>Description</th>
			<th>Owner</th>
			<th>Details</th>
		</tr>
		<% @a.each do |app| %>
		<tr>
			<td><%= app.id %></td>
			<td><%= app.name %></td>
			<td>Pass</td>
			<td><%= app.description %></td>
			<td><%= app.owner %></td>
			<td><%= link_to 'Show', app %></td>
		</tr>
		<% end %>
	</table>
	<%= link_to "Home", controller: "selfservice" %>
</div>
